{
  "name": "react-native-obfuscating-transformer",
  "version": "1.0.0",
  "description": "Obfuscating transformer for react-native",
  "main": "dist/index.js",
  "repository": "https://github.com/ds300/react-native-obfuscating-transformer",
  "author": "David Sheldrick",
  "license": "MIT",
  "dependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/babel__core": "^7.1.14",
    "@types/babel__generator": "^7.6.2",
    "@types/node": "^14.14.37",
    "@types/semver": "^7.3.4",
    "app-root-path": "^2.0.1",
    "@babel/generator": "^7.13.9",
    "@babel/traverse": "^7.13.13",
    "@babel/parser": "^7.13.13",
    "javascript-obfuscator": "^4.0.0",
    "jju": "^1.3.0",
    "semver": "^5.4.1",
    "source-map": "^0.6.1"
  },
  "peerDependencies": {
    "react-native": ">=0.45.0"
  },
  "scripts": {
    "build": "rm -rf dist && tsc",
    "precommit": "tsc && lint-staged",
    "test": "jest"
  },
  "typings": "dist/index.d.ts",
  "lint-staged": {
    "**/*.ts": [
      "prettier --no-semi --trailing-comma all"
    ]
  },
  "devDependencies": {
    "@types/jest": "^22.0.1",
    "babel-preset-react-native": "^4.0.1",
    "husky": "^0.13.3",
    "jest": "^26.0.22",
    "lint-staged": "^3.4.1",
    "metro": "^0.65.2",
    "metro-bundler": "^0.22.1",
    "np": "^4.0.2",
    "prettier": "^1.10.2",
    "react-native": "^0.64.0",
    "ts-jest": "^22.0.1",
    "ts-node": "^4.1.0",
    "typescript": "^4.0.7"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(javascript-obfuscator)/)"
    ],
    "moduleNameMapper": {
      "javascript-obfuscator": "<rootDir>/node_modules/javascript-obfuscator/index.ts"
    },
    "testRegex": "src/.*(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
